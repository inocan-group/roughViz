{
  "name": "rough-viz",
  "version": "1.0.3",
  "description": "Hand drawn, rough, sketchy data visualization in svg.",
  "jsdelivr": "dist/roughviz.min.js",
  "unpkg": "dist/roughviz.min.js",
  "main": "dist/index.js",
  "scripts": {
    "buildUmd": "parcel build src/index.js --target browser --out-file roughviz.min.js --no-source-maps --experimental-scope-hoisting --global roughViz",
    "clean": "rimraf dist/**/*",
    "build": "run-s clean build:autoindex build:bundle",
    "build:autoindex": "yarn do autoindex",
    "build:bundle": "yarn rollup -c",
    "pretestt": "eslint --ignore-path .gitignore .",
    "lint": "eslint --ignore-path .gitignore .",
    "lintFix": "eslint --ignore-path .gitignore . --fix",
    "start": "parcel examples/index.html",
    "test": "jest --env=jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jwilber/roughViz.git"
  },
  "keywords": [
    "chart",
    "graph",
    "rough",
    "hand-drawn",
    "sketchy",
    "dataviz",
    "data visualization"
  ],
  "author": "jwilber",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jwilber/roughViz/issues"
  },
  "homepage": "https://github.com/jwilber/roughViz#readme",
  "dependencies": {
    "native-dash": "^1.2.7"
  },
  "optionalDependencies": {
    "d3": "6",
    "roughjs": "4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/d3": "^5.7.2",
    "babel-eslint": "^10.1.0",
    "d3": "6",
    "do-devops": "^0.4.11",
    "eslint": "^7.9.0",
    "eslint-config-strongloop": "^2.1.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "prettier-eslint": "^11.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.28.2",
    "rollup-plugin-analyzer": "^3.3.0",
    "roughjs": "4"
  }
}